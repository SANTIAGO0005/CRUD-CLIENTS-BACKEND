{
  "name": "ProQuery",
  "version": "0.1.0",
  "private": true,
  "description": "ProQuery-Front-end for conection servers",
  "author": "SANTIAGO CIFUENTES DE LA HOZ",
  "main": "./public/main.js",
  "homepage": ".",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.8",
    "@mui/material": "^5.14.8",
    "@mui/styles": "^5.14.9",
    "@mui/x-tree-view": "^6.0.0-alpha.2",
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.5.0",
    "mui-datatables": "^4.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "sweetalert2": "^11.7.27",
    "web-vitals": "^2.1.4"
  },
  "build": {
    "appId": "ProQuery.UI",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "files": [
      "build/**/*",
      "electron/main.cjs",
      "node_modules/**/*",
      "package.json"
    ],
    "nsis": {
      "uninstallDisplayName": "Uninstall this app",
      "oneClick": "false",
      "allowToChangeInstallationDirectory": "true"
    },
    "directories": {
      "output": "releases",
      "buildResources": "dist"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test-react": "react-scripts test --env=jsdom",
    "eject-react": "react-scripts eject",
    "electron:build": "npm run build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron": "concurrently -k \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "package": "react-scripts build && cross-env ELECTRON_IS_DEV=0 electron-forge package",
    "make": "react-scripts build && cross-env ELECTRON_IS_DEV=0 electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "electron": "^26.2.1",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "electron-builder": "^24.6.4",
    "electron-prebuilt-compile": "8.2.0",
    "wait-on": "^7.0.1"
  }
}
